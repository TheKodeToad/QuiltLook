{
	"schema_version": 1,
	"quilt_loader": {
		"group": "io.github.thekodetoad.quiltlook",
		"id": "quitlook",
		"version": "${version}",
		"metadata": {
			"name": "QuiltLook",
			"description": "Configurable Freelook mod on Quilt.",
			"contributors": {
				"TheKodeToad": "Owner"
			},
			"contact": {
				"homepage": "https://example.com/",
